<table class="table table-striped table-bordered table-sm table-hover followup" style="width: 100%;">
        <caption style="text-align:center;caption-side:top;color:#0275d8;font-size:medium;font-weight:bolder">Delivery Stors</caption>
        <thead class="badge-primary">
            <tr>
                <th>FSA</th>
                <th>Store Number</th>
                <th>Delivery Vendor Id</th>
                <th>Delivery Vendor Name</th>
                <th>Delivery Fee PLU</th>
                <th>Delivery Fee Promo</th>
                <th>Client Code</th>
            </tr>
        </thead>
        <tbody>

            @foreach (var deliveryStore in Model.Documents)
            {


                <tr>
                    <td align="center">

                        <div asp-route-Id="@deliveryStore.Id" method="post">

                            <div>
                                <a style="text-decoration:underline;text-align:left" asp-action="EditDeliveryStore" asp-controller="Home"
                                   asp-route-Id=@deliveryStore.Id>
                                    <b>@deliveryStore.FSA</b>
                                </a>
                            </div>

                        </div>


                    </td>

                    <td>
                        <div align="center">
                            <span class="col-sm-2 col-form-label">@deliveryStore.StoreNumber</span>
                        </div>
                    </td>

                    <td>
                        <div align="center">
                            <span class="col-sm-2 col-form-label">@deliveryStore.DeliveryVendorId</span>
                        </div>
                    </td>


                    <td>
                        <div align="center">
                            <span class="col-sm-2 col-form-label">@deliveryStore.DeliveryVendorName</span>
                        </div>
                    </td>

                    <td>
                        <div align="center">
                            <span class="col-sm-2 col-form-label">@deliveryStore.DeliveryFeePLU</span>
                        </div>
                    </td>

                    <td>
                        <div align="center">
                            <span class="col-sm-2 col-form-label">@deliveryStore.DeliveryFeePromo</span>
                        </div>
                    </td>


                    <td>
                        <div align="center">
                            <span class="col-sm-2 col-form-label">@deliveryStore.ClientCode</span>
                        </div>
                    </td>




                </tr>



            }

        </tbody>
    </table>

